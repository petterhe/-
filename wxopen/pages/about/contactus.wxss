/* pages/about/contactus.wxss */
Page
{
  background-color: #f3f3f3;
}

.content2
{
  margin-top: 30px;
  text-align: center;
  font-size: 13px;
  color: #444;
}

.account
{
  margin-bottom: 10px;
}

.contact_view1
{
  width: 750rpx;
  /* margin: 10px 0 0 0; */
  background-color: #fff;
}

.contact_item1
{
  margin: 0 16px;
  padding: 16px 0;
}

.contact_name1
{
  font-size: 16px;
  color: #000;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}



.contact_view
{
  width: 690rpx;
  margin: 40px 30rpx 0 30rpx;
  background-color: #fff;
  border-radius: 5px;
}

.contact_item
{
  margin: 0 10px;
  padding: 10px 0;
}

.contact_item_line
{
   border-bottom:#f3f3f3 solid 1rpx;
}

.contact_name
{
  font-size: 14px;
  color: #444;
  display: flex;
  flex-direction: row;
  justify-content: space-between;
}

.contact_memo
{
  margin-top: 5px;
  font-size: 12px;
  color: #999;
}

.contact_value
{
  color: #F39800;
}

.contact_online
{
  margin:60px 20px 0 20px;
}
