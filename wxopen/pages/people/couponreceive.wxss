  text-align: right;
  font-size: 10px;
  color: #FFD801;
}

.item_line_image
{
  height: 1px;
  margin: 0 10px;
  position: absolute;
}

.coupon_memo
{
  font-size: 12px;
  margin: 0 10px;
  padding: 30px 0;
}

.coupon_memo_line
{
  display: flex;
  flex-direction: row;
  justify-content: space-betSween;
  margin-bottom: 10px;
}

.coupon_memo_title 
{
  height: 40px;
  line-height:40px;
  text-align:center;
  font-size:15px;
  font-weight: 500;
