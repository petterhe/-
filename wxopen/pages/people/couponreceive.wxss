  font-family:PingFang-SC-Bold;
  letter-spacing:2px;
  background-color: #e6113e;
  border-radius:5px
}

.share_button
{
  width: 180px;
  margin: 30px auto 0 auto;
  height:40px;
  line-height:39px;
  text-align:center;
  color: #fff;
  font-size: 18px; 
  font-family:PingFang-SC-Bold;
  letter-spacing:2px;
