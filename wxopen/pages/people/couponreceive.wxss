  display: flex;
  flex-direction: row;
  justify-content: space-betSween;
}

.avatar_img
{
  margin: 9px 0;
  width: 36px;
  height: 36px;
  border-radius: 18px;
  border:1rpx #fff solid;
}

.receive_list_item_name
{
  height: 24px;
  line-height: 24px;
  margin-left: 8px;
  margin-top: 7px;
  color: #fff;
}

.receive_list_item_time
{
  height: 16px;
  line-height: 16px;
  font-size: 12px;
  margin-left: 8px;
  color: #fff;
}

.receive_list_item_amtview
{
  font-size:16px;
  font-weight: 500;
  color: #fff;
  width: 80px;
}

.receive_list_item_amt
{
  margin-top: 14px;
  height: 24px;
  line-height: 24px;
  font-size:16px;
  font-weight: 500;
  color: #fff;
  width: 80px;
  text-align: right;
}

.item_zuijia_img
{
  width: 10px;
  height: 12.5px;
}

.item_zuijia_view
{
  height: 14px;
